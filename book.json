{ 
    "plugins": [
        "highlight",
        "search",
        "disqus",
        "add-header",
        "anchorjs",
        "asciinema",
        "youtubex",
        "wide-page",
        "word-count",
        "toggle-chapters",
        "todo",
        "theme-packt",
        "tasklist",
        "tabs",
        "spoiler",
        "signature",
        "scrollablecode",
        "richquotes",
        "retina",
        "richquotesbooster",
        "versions",
        "-theme-default",
    ],
},
    "pluginsConfig": {
        "lunr": {
            "maxIndexSize": 1000000,
            "ignoreSpecialCharacters": false
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
        "disqus": {
            "shortName": "fermat-wiki",
            "useIdentifier": true
        },
        "versions": {
            "type": "branches",
            "includeFilepath": true
        }
    }
}