{
    "plugins": [
        "highlight",
        "search",
        "disqus",
        "add-header",
        "anchorjs",
        "asciinema",
        "youtubex",
        "wide-page",
        "word-count",
        "toggle-chapters",
        "todo",
        "theme-packt",
        "tasklist",
        "tabs",
        "spoiler",
        "signature",
        "scrollablecode",
        "richquotes",
        "retina",
        "versioning",
        "richquotesbooster",
        "versions",
        "-theme-default"
    ],
    "pluginsConfig": {
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": true,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": "facebook,google,twitter"
        },
        "lunr": {
            "maxIndexSize": 1000000,
            "ignoreSpecialCharacters": false
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
        "theme-default": {
            "styles": "",
            "showLevel": true
        },
        "disqus": {
            "shortName": "fermat-wiki",
            "useIdentifier": true
        },
        "versions": {
            "type": "branches",
            "includeFilepath": true
        },
    }
}